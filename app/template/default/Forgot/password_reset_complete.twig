{% extends 'default_frame.twig' %}

{% block main %}
      <div class="wrapper">
        <div class="breadcrumbs">
          <div class="breadcrumbs__page-title">
            <div class="breadcrumbs__page-title-image"><img src="/html/template/default/assets/img/icons/page-title-logo.png" alt=""></div>
            <div class="breadcrumbs__page-title-text">
              <div class="breadcrumbs__page-title-text-en">{{'front.mypage.password_reset.subtitle'|trans}}</div>
              <div class="breadcrumbs__page-title-text-jp">{{'front.mypage.password_reset.title'|trans}}</div>
            </div>
          </div>
          <div class="breadcrumbs__main"><a class="breadcrumbs__main-pagename breadcrumbs__main-pagename--inactive" href="https://captainfoods.co.jp/stg/shop/">{{'common.position_top'|trans}}</a>
            <div class="breadcrumbs__main-carret"><img src="/html/template/default/assets/img/icons/icon-arrow-right-red.svg" alt=""></div><span class="breadcrumbs__main-pagename">{{'common.position_password_reset'|trans}}</span>
          </div>
        </div>
      </div>
      <section class="login"> 
        <div class="login__content"> 
          <div class="login__wrap"> 
            <h3 class="login__title">{{'front.mypage.password_reset_complete.title'|trans}}</h3>
            <div class="login__container">
              <div class="login__button-container">
                <button class="button__pamphlet login__button-p">{{'common.move_to_page_login'|trans}}</button>
                <button class="button__pamphlet login__button-p">{{'common.go_to_top'|trans}}</button>
              </div>
            </div>
          </div>
        </div>
      </section>

{% endblock %}

